  <div class="about-container">
    <div class="white-bg-block"></div>

    <div class="about-title-block" data-directus={setAttr?.({ collection: 'blocks', item: block.id, fields: [`au_title_line1_${locale}`, `au_title_line2_${locale}`], mode: 'popover' })}>
      <p class="title-line">{titleLine1}</p>
      <p class="title-line">{titleLine2}</p>
    </div>

    <p class="about-intro" data-directus={setAttr?.({ collection: 'blocks', item: block.id, fields: [`au_intro_${locale}`], mode: 'popover' })}>{introText}</p>


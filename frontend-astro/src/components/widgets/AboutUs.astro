---
import { setAttr } from '../../lib/directus';

interface Props {
  block: any;
  locale: string;
}

const { block, locale } = Astro.props;

const titleLine1 = block?.[`au_title_line1_${locale}`] || 'Let us introduce';
const titleLine2 = block?.[`au_title_line2_${locale}`] || 'ourselves';
const introText = block?.[`au_intro_${locale}`] || 'We created Oddny to deal with the boring but necessary stuff';
const missionTitle = block?.[`au_mission_title_${locale}`] || 'Our Mission';
const missionText = block?.[`au_mission_text_${locale}`] || 'Once, a Wunderkammer gathered the wonders of art, nature and invention in one place.\n\nAt Oddny, we carry that spirit forward — exploring, connecting and sharing insights from the creative and financial worlds to protect, inspire and expand what creativity can achieve.\n\nA space to discover, to be inspired, and to spark new possibilities.';
const promiseTitle = block?.[`au_promise_title_${locale}`] || 'Our Promise';
const promiseText = block?.[`au_promise_text_${locale}`] || 'Once, a Wunderkammer gathered the wonders of art, nature and invention in one place.\n\nAt Oddny, we carry that spirit forward — exploring, connecting and sharing insights from the creative and financial worlds to protect, inspire and expand what creativity can achieve.\n\nA space to discover, to be inspired, and to spark new possibilities.';
const ctaText = block?.[`au_cta_text_${locale}`] || 'Get to know us some more...';
const ctaLink = block?.[`au_cta_link_${locale}`] || '/about';
---


interface Props {
  block: any;
  locale: string;
  getImageUrl: (id: string) => string;
  setAttr: (opts: any) => string;
}

const { block, locale, getImageUrl, setAttr } = Astro.props;

const getLocalizedField = (block: any, fieldName: string) => {
  const localizedFieldName = `${fieldName}_${locale}`;
  return block[localizedFieldName] || block[fieldName] || '';
};

const email = block.contact_email || 'contact@oddny.nl';
const phone = block.contact_phone || '0900 333 355 553';
const titleText = getLocalizedField(block, 'title_text') || "If you'd like to say hello...";
const emailText = getLocalizedField(block, 'email_text') || '...you can fling us an email...';
const phoneText = getLocalizedField(block, 'phone_text') || '...dig out our phone number and give us a call...';
const postcardText = getLocalizedField(block, 'postcard_text') || '...or even send us a postcard if you like...';


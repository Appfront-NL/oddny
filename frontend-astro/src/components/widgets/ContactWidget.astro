<section 
  class="contact-widget-section"
  data-directus={setAttr({ collection: 'blocks', item: block.id, mode: 'drawer' })}
>
  <style>
    .contact-widget-section {
      width: 100vw;
      margin-left: calc(-50vw + 50%);
      background: white;
      font-family: 'Neue Haas Grotesk Display Pro', 'Helvetica Neue', Helvetica, Arial, sans-serif;
      overflow: visible;
      position: relative;
    }
    
    .contact-desktop {
      display: none;
    }
    
    .contact-mobile {
      display: flex;
      flex-direction: column;
      padding: 40px var(--content-padding);
      gap: 48px;
      min-height: 100vh;
    }
    
    .contact-mobile-header {
      display: flex;
      flex-direction: column;
      align-items: center;
      margin-bottom: 24px;
      position: relative;
    }
    
    .contact-mobile-header h1 {
      color: #FF40B4;
      font-size: 48px;
      font-weight: 700;
      z-index: 10;
      margin-top: 40px;
    }
    
    .contact-mobile-decor {
      position: absolute;
      top: -40px;
      left: -40px;
      opacity: 0.5;
      transform: rotate(12deg);
      width: 160px;
      height: 160px;
    }
    
    .contact-section-item {
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 24px;
      text-align: center;
      max-width: 400px;
      margin: 0 auto;
      width: 100%;
      position: relative;
    }
    
    .contact-text {
      color: #888484;
      font-size: 20px;
      line-height: 28px;
      font-weight: 300;
      max-width: 300px;
    }
    
    .contact-button {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      background: #FF40B4;
      color: white;
      font-size: 18px;
      font-weight: 700;
      padding: 14px 32px;
      border-radius: 50px;
      text-decoration: none;
      transition: background 0.2s ease;
      box-shadow: 0 4px 15px rgba(255, 64, 180, 0.3);
    }
    
    .contact-button:hover {
      background: #d63396;
    }
    
    .contact-button svg {
      width: 20px;
      height: 20px;
    }
    
    .contact-image {
      width: 192px;
      height: auto;
      object-fit: contain;
    }
    
    .contact-handset-mobile {
      width: 192px;
      height: 192px;
      position: relative;
      margin: 0 auto;
    }
    
    .contact-bird-mobile {
      width: 224px;
      height: auto;
    }
    
    .contact-footer-decor {
      display: none;
    }
    
    .contact-footer-decor img {
      position: absolute;
      width: 100%;
      height: 100%;
      object-fit: cover;
      opacity: 0.3;
      transform: rotate(180deg);
    }
    
    @media (min-width: 768px) and (max-width: 1023px) {
      .contact-mobile {
        padding: 60px var(--content-padding);
        gap: 64px;
      }
      
      .contact-mobile-header h1 {
        font-size: 64px;
      }
      
      .contact-section-item {
        max-width: 500px;
        gap: 32px;
      }
      
      .contact-text {
        font-size: 24px;
        line-height: 32px;
        max-width: 400px;
      }
      
      .contact-button {
        font-size: 20px;
        padding: 16px 40px;
      }
      
      .contact-image {
        width: 256px;
      }
      
      .contact-handset-mobile {
        width: 256px;
        height: 256px;
      }
      
      .contact-bird-mobile {
        width: 300px;
      }
    }
    
    @media (min-width: 1024px) {
      .contact-mobile {
        display: none;
      }
      
      .contact-desktop {
        display: flex;
        justify-content: center;
        width: 100%;
        position: relative;
      }
      
      .contact-desktop-inner {
        position: relative;
        width: 100%;
        max-width: var(--content-max-width);
        aspect-ratio: 1440 / 1900;
      }
      
      .contact-title {
        position: absolute;
        top: 1%;
        left: 50%;
        transform: translateX(-50%);
        font-size: 24px;
        font-weight: 500;
        color: #888484;
        z-index: 100;
        white-space: nowrap;
        font-family: 'Neue Haas Grotesk Display Pro', sans-serif;
      }
    }
    
    @media (min-width: 1024px) {
      .contact-bg-top {
        position: absolute;
        background: rgba(217, 217, 217, 0.72);
        height: 36.95%;
        left: 50%;
        transform: translateX(-50%);
        top: -4.2%;
        width: 100vw;
      }
      
      .contact-bg-bottom {
        position: absolute;
        background: rgba(217, 217, 217, 0.72);
        height: 45.21%;
        left: 50%;
        transform: translateX(-50%);
        top: 54.79%;
        width: 100vw;
      }
      
      .contact-layer-1 {
        position: absolute;
        height: 7.29%;
        left: 8.33%;
        top: 76.68%;
        width: 8.64%;
      }
      
      .contact-layer-4-wrap {
        position: absolute;
        display: flex;
        height: 12.55%;
        align-items: center;
        justify-content: center;
        left: 9.17%;
        top: 7.58%;
        width: 15.49%;
      }
      
      .contact-layer-4 {
        transform: rotate(15.032deg);
        width: 100%;
        height: 100%;
      }
      
      .contact-layer-5 {
        position: absolute;
        height: 13.58%;
        left: 50.56%;
        overflow: hidden;
        top: 30.74%;
        width: 23.06%;
      }
      
      .contact-phone-number {
        position: absolute;
        left: 43.75%;
        top: 28.42%;
        width: 13.89%;
        height: 10.53%;
        pointer-events: none;
      }
      
      .contact-phone-link {
        position: absolute;
        inset: 0;
        pointer-events: auto;
        cursor: pointer;
      }
      
      .contact-phone-edit {
        display: none;
      }
      
      .contact-email-edit {
        display: none;
      }
      
      .contact-dashed-line {
        position: absolute;
        height: 15.79%;
        left: 19.44%;
        top: 6.32%;
        width: 62.5%;
        overflow: visible;
        z-index: 50;
      }
      
      .contact-dashed-line-inner {
        position: relative;
        width: 100%;
        height: 100%;
        overflow: visible;
      }
      
      .contact-dashed-line svg {
        overflow: visible;
      }
      
      .contact-email-link {
        position: absolute;
        left: 5%;
        top: 40%;
        width: 40%;
        height: 40%;
        cursor: pointer;
        z-index: 100;
        display: block;
      }
      
      .sr-only {
        position: absolute;
        width: 1px;
        height: 1px;
        padding: 0;
        margin: -1px;
        overflow: hidden;
        clip: rect(0, 0, 0, 0);
        white-space: nowrap;
        border: 0;
      }
      
      .contact-email-edit-overlay {
        display: block;
        width: 100%;
        height: 100%;
        color: transparent;
        cursor: pointer;
      }
      
      .contact-jumping-bear {
        position: absolute;
        height: 13.68%;
        left: 71.81%;
        top: 20.21%;
        width: 18.75%;
      }
      
      .contact-jumping-bear img {
        position: absolute;
        inset: 0;
        width: 100%;
        height: 100%;
